
public class Nested_loops {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int k =1;
		
		for (int i=1; i<=4; i++)
		{
			//System.out.println(i);
		
		
			for( int j=1; j<=4-i; j++)
			{
				//System.out.println(j);
				System.out.print(k);
				System.out.print("\t");
				k++;
			}
			System.out.print("\n a \n");
		}
		
	}

}
