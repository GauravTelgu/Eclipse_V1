
public class OddEven_18 {

	public void OddEven() {
		
		int num1 = 0;
		int num2 = 0;
		
		if(num1 % num2 ==0) {
			System.out.println("Even Number");
		}
		else {
			System.out.println("Odd Number");
		}
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
