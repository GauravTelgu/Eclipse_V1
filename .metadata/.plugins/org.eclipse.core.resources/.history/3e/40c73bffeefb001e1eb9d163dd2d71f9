
public class Nested_loops1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		for (int i=1; i<=4; i++)				// rows
		{
			//System.out.println(i);
		
		
			for( int j=1; j<=i; j++)				// columns
			{
				System.out.print(j);
				//System.out.print(j);
						
				System.out.print("\t");	
				
				//2
			}
			
			
			System.out.print("\n");
		}	
		
	}

}
