package Collections;

import java.util.HashMap;

public class hashmapexample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		HashMap<Integer, String> hm = new HashMap<Integer, String>();
		hm.put(0, "Gaurav");
		hm.put(1, "Telgu");
		hm.put(2, "Java");
		hm.put(3, "Selenium");
		
	}

}
