package Test;

public class Practicearray {
		
	public static void main(String[] args) {
	     int[][] myNumbers = { 
	     						{1, 2, 3, 4}, 
	     						{5, 6, 7} 
	                            };
	                            
	     for (int i = 0; i < myNumbers.length; i++)    //outer loop
	    {
	     for(int j = 0; j < myNumbers[i].length; j++) 	// inner loop
	        {
	           System.out.print(myNumbers[i][j]+ " ");
	           
	        }
	         System.out.println();
			
	    }
	}
}


