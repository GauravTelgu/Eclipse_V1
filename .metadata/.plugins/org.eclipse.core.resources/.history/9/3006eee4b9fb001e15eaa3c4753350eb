
public class Nested_loops {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	//	int k =1;
		
		for (int i=1; i<4; i++)
		{
			//System.out.println(i);
		
		
			for( int j=1; j<=4; j++)				// 1
			{
				//System.out.println(j);
				System.out.print(j);				//1
				System.out.print("\t");
				//k++;								//2
			}
			System.out.print("\n");
		}
		
	}

}
