
public class While_For_loop_16 {

	public void while_loop() {
		int i=0;
		while (i<100) {
			System.out.println("Print 100 numbers:");
			i++;
			
		}
	}
	
	public void for_loop() {
		int i=0;
		while (i<100) {
			System.out.println("Print 100 numbers:");
			i++;
			
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
