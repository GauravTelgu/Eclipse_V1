package Test;

public class Array {

	public static void main (String args[]) {
	int [] s= new int[25];
	int [] a= new int[] {1,2,3,4,5};
	int b[]= {5,6,7,8,9};
	int [][] multi = {
		{1,7},
		{3,4,5},
		{5,6}
					};
	System.out.println(a[2]);
	System.out.println(b[3]);
	System.out.println(s.length);
	System.out.println(multi[0][1]);
	}
}
