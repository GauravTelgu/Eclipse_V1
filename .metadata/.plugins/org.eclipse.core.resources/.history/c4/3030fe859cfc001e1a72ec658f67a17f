
public class Nested_loops1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		/*
		 * int k = 1;
		 * 
		 * 
		 * for (int i=1; i<=4; i++) // rows { //System.out.println(i);
		 * 
		 * 
		 * for( int j=1; j<=i; j++) // columns { System.out.print(j);
		 * //System.out.print(i); //k++;
		 * 
		 * System.out.print("\t");
		 * 
		 * //2 }
		 * 
		 * 
		 * System.out.print("\n"); }
		 */
		
		for (int i=1; i<=3; i++)
		{
			for (int j=1; j<=i; j++)
			{
				System.out.print("j");
				System.out.print("\t");
			}
				 
				 System.out.print("\n"); }
				 
			}
		}
	

