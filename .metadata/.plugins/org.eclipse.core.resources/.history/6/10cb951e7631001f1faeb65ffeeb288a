
public class OddEven_18 {

	public void OddEven() {
		
		int num1 = 50;
		int num2 = 100;
		
		if(num1 % num2 ==0) {
			System.out.println("Even Number");
		}
		else {
			System.out.println("Odd Number");
		}
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		OddEven_18 obj1 =new OddEven_18();
		obj1.OddEven();
		
	}

}
